{
  "name": "ecomm_back",
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Attach To npm",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "address": "localhost",
      "restart": false,
      "sourceMaps": false,
      "outDir": null,
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null
    },
    {
      "name": "Launch via NPM",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "port": 5858
    }
  ],
  "description": "Backend with Node.js created by Hari Gupta(8470874534) 30thMay2020",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "debug": "node --nolazy --inspect-brk=5858 app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hari2013gupta/ecomm_back.git"
  },
  "keywords": [
    "Node.Js",
    "jwt",
    "rest",
    "api",
    "authentication",
    "express",
    "sequelize",
    "ecommerce",
    "upload",
    "mysql"
  ],
  "author": "Hariom Gupta",
  "license": "ISC",
  "dependencies": {
    "MD5": "~1.2.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "ejs": "^2.7.4",
    "express": "^4.17.3",
    "express-validator": "^6.5.0",
    "jsonfile": "6.0.1",
    "jsonwebtoken": "^5.4.1",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "nodemailer": "latest",
    "sendotp": "^1.2.9",
    "sequelize": "^5.21.3"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "nodemon": "^2.0.5"
  },
  "bugs": {
    "url": "https://github.com/hari2013gupta/ecomm_back/issues"
  },
  "homepage": "https://github.com/hari2013gupta/ecomm_back#readme"
}
